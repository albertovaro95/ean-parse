  "bugs": {
    "url": "https://github.com/albertovaro95/ean-parse/issues"
  },
  "homepage": "https://github.com/albertovaro95/ean-parse#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/source-map-support": "^0.5.4",
    "jest": "^27.5.1",
    "onchange": "^7.1.0",
    "serve": "^13.0.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "ts-node": "^10.7.0"
  }
